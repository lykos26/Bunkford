<addons>
<addon id="plugin.audio.googlemusic"
       name="Google Music Dev"
       version="0.8exp10"
       provider-name="Valentin-Costel Haloiu">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="xbmc.addon" version="12.0.0"/>
    <import addon="script.module.parsedom" version="2.5.1"/>
    <import addon="script.module.validictory" version="0.9.0"/>
    <import addon="script.module.decorator" version="3.3.3"/>
    <import addon="script.module.requests" version="1.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <platform>all</platform>
    <summary lang="en">Google Music audio plugin</summary>
    <description lang="en">Plugin that lets you play music from Google Music.</description>
    <disclaimer>Some parts of this addon may not be legal in your country of residence - please check with your local laws before installing.</disclaimer>
  </extension>
</addon>

<addon
	id="plugin.video.bunkford"
	name="Bunkford"
	version="1.4.9"
	provider-name="bunkford">

  <requires>
	  <import addon="xbmc.python" version="2.1.0"/>
	  <import addon="script.module.beautifulsoup" version="3.0.8" />
	  <import addon="script.module.urlresolver"  version="1.0.0"/>
	  <import addon="script.module.t0mm0.common" version="1.1.0"/>
	  <import addon="script.module.metahandler"  version="1.0.0"/>
	  <import addon="script.module.requests"  version="1.1.0"/>
  </requires>

  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>

  <extension point="xbmc.addon.metadata">
    <summary lang="en">Video Plugin maintained by Bunkford</summary>

    <description lang="en">
	    A video plugin maintaned by bunkford for sites such as iserie.es, barwo.com, putlockertvshows.me, bunnymovie.com. These are one click sources, so no messing around with bad links.

		As far as I know you can only access content off these sites on iDevices, but you can go to the following links on any computer and register.
		Then put in your login and password in plugin settings.

		Go to http://iseri.es/wp-login.php?action=register to register for iseri.es
		Go to http://barwo.com/wp-login.php?action=register to register for barwo.com  (CURRENTLY CLOSED)

		** barwo.com is switching to bunnymovies.com.. go to http://bunnymovie.com/wp-login.php?action=register to register now, support coming in the future.

    </description>


    <disclaimer lang="en">use at your own risk!</disclaimer>

    <license>GNU GENERAL PUBLIC LICENSE. Version 3, June 2007</license>

    <platform>all</platform>

    <language>en</language>

    <website></website>

    <source></source>
  </extension>
</addon>

<addon id="plugin.video.tvlinks" name="TVLinks.cc" provider-name="bunkford" version="1.0.0">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
        <import addon="script.module.urlresolver"  version="1.0.0"/>
		<import addon="script.module.metahandler"  version="1.0.0"/>
    </requires>
  <extension library="default.py" point="xbmc.python.pluginsource">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
	<summary lang="en">TVLinks Plugin</summary>
    <description lang="en">Plugin that lets you browse and play Free Movies from TVLinks.cc! Login is required, change username and password in settings. </description>
    <disclaimer lang="en">Some parts of this addon may not be legal in your country of residence - please check with your local laws before installing.</disclaimer>
    <platform>all</platform>
    <language />
  </extension>
</addon>

<addon id="repository.bunkford" name="Bunkford's Repository" version="1.1" provider-name="bunkford">
	<extension point="xbmc.addon.repository" name="Bunkford's Repository">
		<info compressed="false">http://github.com/bunkford/Bunkford/raw/master/addons.xml</info>
		<checksum>http://github.com/bunkford/Bunkford/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">http://github.com/bunkford/Bunkford/raw/master/zips</datadir>
	</extension>
    <extension point="xbmc.addon.metadata">
        <summary>bunkford's Addon Repository</summary>
        <description>Download and install bunkford's addon repository, home of bunkford, putlockertv and the new tvlinks.cc Plugins!</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
    </extension>
</addon>

